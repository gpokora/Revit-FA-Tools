  "calculationFormulas": {
    "voltageDrop": {
      "formula": "VD = (2 × L × R × I) / 1000",
      "variables": {
        "VD": "Voltage drop (V)",
        "L": "One-way wire length (ft)",
        "R": "Wire resistance (ohms/1000ft)",
        "I": "Current (A)"
      },
      "note": "Multiply by 2 for round-trip resistance"
    },
    "minimumVoltage": {
      "formula": "Vmin = Vsupply - VD",
      "variables": {
        "Vmin": "Minimum voltage at device",
        "Vsupply": "Supply voltage (24V typical, 29V for IDNAC)",
        "VD": "Total voltage drop"
      }
    },
    "totalCircuitCurrent": {
      "formula": "Itotal = Σ(In × Qn) × SF",
      "variables": {
        "Itotal": "Total circuit current",
        "In": "Individual device current",
        "Qn": "Quantity of device type n",
        "SF": "Safety factor (1.2 for 20% spare)"
      }
    },
    "unitLoadCalculation": {
      "formula": "UL = Σ(Dn × ULn)",
      "variables": {
        "UL": "Total unit loads",
        "Dn": "Number of devices of type n",
        "ULn": "Unit loads per device type"
      },
      "note": "Each unit load = 0.8mA standby current"
    },
    "speakerPower": {
      "formula": "P = V² / Z",
      "variables": {
        "P": "Power (watts)",
        "V": "Voltage (70.7V or 25V)",
        "Z": "Speaker impedance at tap setting"
      }
    },
    "amplifierLoad": {
      "formula": "Ptotal = Σ(Sn × Tn)",
      "variables": {
        "Ptotal": "Total amplifier load (watts)",
        "Sn": "Number of speakers at tap n",
        "Tn": "Tap wattage setting"
      }
    },
    "batteryCapacity": {
      "formula": "AH = [(Istandby × Tstandby) + (Ialarm × Talarm / 60)] × 1.2",
      "variables": {
        "AH": "Required battery capacity (amp-hours)",
        "Istandby": "Standby current (A)",
        "Tstandby": "Standby time (hours)",
        "Ialarm": "Alarm current (A)",
        "Talarm": "Alarm time (minutes)"
      },
      "nfpaRequirements": {
        "standardStandby_hours": 24,
        "standardAlarm_minutes": 5,
        "voiceStandby_hours": 24,
        "voiceAlarm_minutes": 15
      }
    },
    "idnacsRequired": {
      "formula": "N = MAX(Ncurrent, Ndevices, Nunitloads)",
      "variables": {
        "N": "Number of IDNACs required",
        "Ncurrent": "CEIL(Itotal / 2.4A)",
        "Ndevices": "CEIL(Devices / 101)",
        "Nunitloads": "CEIL(UnitLoads / 111)"
      }
    }
  },

