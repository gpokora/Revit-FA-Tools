  "parameterMappingRules": {
    "candelaExtraction": {
      "patterns": ["(\\d+)\\s*(?:CD|CANDELA|CANDELAS)\\b"],
      "validValues": [15, 30, 75, 95, 110, 135, 177, 185],
      "defaultValue": 75
    },
    "wattageExtraction": {
      "patterns": ["(\\d+(?:\\.\\d+)?)\\s*(?:W|WATT|WATTS)\\b"],
      "validRange": [0.1, 100.0],
      "defaultValue": 1.0
    },
    "frequencyExtraction": {
      "patterns": ["(\\d+)\\s*(?:HZ|HERTZ)\\b"],
      "validValues": [400, 520, 800],
      "defaultValue": 400,
      "specialHandling": {
        "520": {
          "unitLoads": 2,
          "description": "Special 520Hz frequency requires 2 unit loads"
        }
      }
    }
  },
